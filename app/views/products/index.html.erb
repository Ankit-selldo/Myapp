<h1><%= t ".title" %></h1>

<% if authenticated? %>
  <%= link_to "New product", new_product_path %>
<% else %>
  <%= link_to "Login", new_session_path %>
<% end %>

<div id="products">
  <% @products.each do |product| %>
    <div>
        <%= link_to product.name, product %>
    </div>
  <% end %>
</div>
